{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://unemployment-claims-backend.onrender.com/api/:path*"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "https://unemployment-claims-backend.onrender.com/api"
  }
} 